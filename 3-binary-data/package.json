    "nconf": "0.8.4",
    "prompt": "1.0.0"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "nodejs-repo-tools": "git+https://git@github.com/ace-n/nodejs-repo-tools.git",
    "proxyquire": "1.7.10",
    "sinon": "1.17.7",
    "tap-dot": "1.0.5"
  },
  "engines": {
    "node": ">=4.3.2"
  }
}
