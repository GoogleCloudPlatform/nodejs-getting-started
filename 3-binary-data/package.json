    },
    {
      "name": "Jonathan Simon",
      "email": "jbsimon@google.com"
    },
    {
      "name": "Jason Dobry",
      "email": "jdobry@google.com"
    }
  ],
  "license": "Apache Version 2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "0.6.1",
    "@google-cloud/storage": "0.6.1",
    "body-parser": "1.16.0",
    "express": "4.14.0",
    "jade": "1.11.0",
    "lodash": "4.17.4",
    "mongodb": "2.2.21",
    "multer": "1.2.1",
