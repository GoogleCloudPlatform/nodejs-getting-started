    "Steve Perry <steveperry-53@users.noreply.github.com>",
    "chenyumic <chenyumic@google.com>"
  ],
  "scripts": {
    "lint": "semistandard \"**/*.js\"",
    "ava": "ava -s -t 30s --tap \"**/!(datastore).test.js\"",
