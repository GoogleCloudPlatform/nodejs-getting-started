{
  "name": "nodejs-getting-started",
  "version": "1.0.0",
  "description": "End to end sample for running Node.JS applications on Google Cloud Platform",
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
  "private": true,
  "scripts": {
    "lint": "semistandard \"**/*.js\"",
    "ava": "ava -s -t 30s --tap 1-hello-world/test/*.test.js 2-structured-data/test/*.test.js 3-binary-data/test/*.test.js | tap-dot",
    "test": "nyc ava */test/index.js --timeout=10s | tap-dot",
    "e2e": "mocha test/e2e.js -t 1800000",
    "codecov": "cat ./coverage/lcov.info | codecov",
    "unify": "node scripts/unify"
  },
  "author": "Google Inc.",
  "contributors": [
    {
      "name": "Jon Wayne Parrott",
      "email": "jonwayne@google.com"
    },
    {
      "name": "Jonathan Simon",
      "email": "jbsimon@google.com"
    },
    {
      "name": "Jason Dobry",
      "email": "jdobry@google.com"
    }
  ],
  "license": "Apache Version 2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "ava": {
    "files": [
      "1-hello-world/test/*.test.js",
      "2-structured-data/test/*.test.js",
      "3-binary-data/test/*.test.js"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "0.6.1",
    "@google-cloud/storage": "0.6.1",
    "body-parser": "1.16.0",
    "express": "4.14.0",
    "jade": "1.11.0",
    "lodash": "4.17.4",
    "mongodb": "2.2.21",
    "multer": "1.2.1",
    "mysql": "2.12.0",
    "nconf": "0.8.4",
    "prompt": "1.0.0"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "nodejs-repo-tools": "git+https://git@github.com/ace-n/nodejs-repo-tools.git",
    "proxyquire": "1.7.10",
    "shelljs": "0.7.3",
    "sinon": "1.17.7",
    "tap-dot": "1.0.5"
  },
  "engines": {
    "node": ">=4.3.2"
  }
}
