    "connect-memcached": "0.2.0",
    "connect-memjs": "0.2.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-winston": "^2.5.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.2",
