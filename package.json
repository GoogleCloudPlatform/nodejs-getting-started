  "author": "Google Inc.",
  "engines": {
    "node": ">=4.3.2"
  },
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
  "main": "src/index.js",
  "private": true,
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
