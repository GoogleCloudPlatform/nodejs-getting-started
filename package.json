    "async": "^1.5.2",
    "body-parser": "^1.14.2",
    "cookie-session": "^2.0.0-alpha.1",
    "express": "^4.13.4",
    "express-winston": "^1.2.0",
    "gcloud": "^0.27.0",
    "googleapis": "^2.1.7",
    "jade": "^1.11.0",
    "kerberos": "^0.0.18",
    "lodash": "^4.2.1",
    "mongodb": "^2.1.4",
    "multer": "^1.1.0",
    "mysql": "^2.10.2",
    "prompt": "^0.2.14",
    "request": "^2.69.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "supertest": "^1.1.0"
  },
  "engines": {
    "node": ">=0.12.7"
  }
}
