# GCP project configs
function techday-anassri {
    export GOOGLE_APPLICATION_CREDENTIALS="/usr/local/google/home/anassri/Desktop/techday-anassri-d4a5f62a5e48.json"
    export GCLOUD_PROJECT="google.com:techday-anassri"
    [ -s "gcloud config set project $GCLOUD_PROJECT" ]
}
function techday-old {
    export GOOGLE_APPLICATION_CREDENTIALS="/usr/local/google/home/anassri/Desktop/techday-explore.json"
    export GCLOUD_PROJECT="techday"
    [ -s "gcloud config set project $GCLOUD_PROJECT" ]
}
function docs-samples {
    export GOOGLE_APPLICATION_CREDENTIALS="/usr/local/google/home/anassri/nodejs-docs-samples-0351b65f5732.json"
    export GCLOUD_PROJECT="nodejs-docs-samples"
    [ -s "gcloud config set project $GCLOUD_PROJECT" ]
}
function getting-started {
    export GOOGLE_APPLICATION_CREDENTIALS="/usr/local/google/home/anassri/Desktop/anassri-nodejs-starter.json"
    export GCLOUD_PROJECT="anassri-nodejs-starter"
    [ -s "gcloud config set project $GCLOUD_PROJECT" ]
}

