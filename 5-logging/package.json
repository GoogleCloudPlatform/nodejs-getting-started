{
  "name": "nodejs-getting-started",
  "version": "1.0.0",
  "description": "End to end sample for running Node.js applications on Google Cloud Platform",
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "samples test app && ava -t 30s --tap test/*.test.js | tap-dot",
    "e2e": "rm -rf node_modules/ &&  samples test build --deploy --timeout 20m --builderProject $GCLOUD_PROJECT",
    "cover": "nyc --cache npm test; nyc report --reporter=html",
    "init-cloudsql": "node books/model-cloudsql.js"
  },
  "author": "Google Inc.",
  "contributors": [
    {
      "name": "Jon Wayne Parrott",
      "email": "jonwayne@google.com"
    },
    {
      "name": "Jonathan Simon",
      "email": "jbsimon@google.com"
    },
    {
      "name": "Jason Dobry",
      "email": "jdobry@google.com"
    },
    {
      "name": "Ace Nassri",
      "email": "anassri@google.com"
    }
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Bookshelf - Node.js on Google Cloud Platform"
      }
    }
  },
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "~1.1.0",
    "@google-cloud/debug-agent": "~2.1.1",
    "@google-cloud/logging-winston": "^0.5.0",
    "@google-cloud/storage": "~1.2.1",
    "@google-cloud/trace-agent": "~2.1.0",
    "body-parser": "~1.17.1",
    "connect-memcached": "0.2.0",
    "express": "~4.15.2",
    "express-session": "~1.15.2",
    "express-winston": "~2.4.0",
    "pug": "2.0.0-rc.3",
    "lodash": "~4.17.4",
    "mongodb": "~2.2.25",
    "multer": "~1.3.0",
    "mysql": "~2.14.0",
    "nconf": "~0.8.4",
    "passport": "~0.3.2",
    "passport-google-oauth20": "~1.0.0",
    "prompt": "~1.0.0",
    "uglify-js": "3.0.27",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "1.4.16",
    "ava": "~0.21.0",
    "proxyquire": "~1.8.0",
    "sinon": "~3.0.0",
    "supertest": "~3.0.0",
    "tap-dot": "~1.0.5"
  },
  "engines": {
    "node": ">=4.3.2"
  }
}
